/* pages/education/activity/userlist/userlist.wxss */
.userlist{

}
.userlist .head{
  background: rgb(108, 214, 208);
  display: flex;
  justify-content: space-between;
  padding: 30rpx;
  position: fixed;
  box-sizing: border-box;
  left: 0;
  top: 0;
  width: 100%;
}
.userlist .head .head-right{
  display: flex;
  align-items: center;
  font-size: 36rpx;
 
}
.userlist .head .head-left .activity-name{
 font-size: 38rpx;
 margin-bottom: 15rpx;
}
.userlist .head .head-left .activity-date{
 font-size: 30rpx;

}
.userlist-box{
    margin:160rpx 0 50rpx;
}
.userlist-box .user-item{
  padding: 30rpx 80rpx;
  border-bottom: 1px solid rgb(229,229,229);
  display: flex;
  justify-content: space-between;
}
.userlist-box .user-item .item-student .classnumb{
 font-size: 30rpx;
 margin-left: 10rpx;
}